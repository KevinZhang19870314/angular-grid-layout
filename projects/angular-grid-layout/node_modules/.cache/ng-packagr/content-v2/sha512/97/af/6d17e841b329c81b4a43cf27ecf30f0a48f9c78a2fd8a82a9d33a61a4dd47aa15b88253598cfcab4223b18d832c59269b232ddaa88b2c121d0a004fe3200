,"end":10489,"object":{"type":"Identifier","start":10473,"end":10485,"name":"documentRect"},"property":{"type":"Identifier","start":10486,"end":10489,"name":"top"},"computed":false,"optional":false}},"operator":"||","right":{"type":"MemberExpression","start":10493,"end":10516,"object":{"type":"MemberExpression","start":10493,"end":10506,"object":{"type":"Identifier","start":10493,"end":10501,"name":"document"},"property":{"type":"Identifier","start":10502,"end":10506,"name":"body"},"computed":false,"optional":false},"property":{"type":"Identifier","start":10507,"end":10516,"name":"scrollTop"},"computed":false,"optional":false}},"operator":"||","right":{"type":"MemberExpression","start":10520,"end":10537,"object":{"type":"Identifier","start":10520,"end":10529,"name":"windowRef"},"property":{"type":"Identifier","start":10530,"end":10537,"name":"scrollY"},"computed":false,"optional":false}},"operator":"||","right":{"type":"MemberExpression","start":10550,"end":10575,"object":{"type":"Identifier","start":10550,"end":10565,"name":"documentElement"},"property":{"type":"Identifier","start":10566,"end":10575,"name":"scrollTop"},"computed":false,"optional":false}},"operator":"||","right":{"type":"Literal","start":10579,"end":10580,"value":0,"raw":"0"}}}],"kind":"const"},{"type":"VariableDeclaration","start":10587,"end":10712,"declarations":[{"type":"VariableDeclarator","start":10593,"end":10711,"id":{"type":"Identifier","start":10593,"end":10597,"name":"left"},"init":{"type":"LogicalExpression","start":10600,"end":10711,"left":{"type":"LogicalExpression","start":10600,"end":10706,"left":{"type":"LogicalExpression","start":10600,"end":10667,"left":{"type":"LogicalExpression","start":10600,"end":10646,"left":{"type":"UnaryExpression","start":10600,"end":10618,"operator":"-","prefix":true,"argument":{"type":"MemberExpression","start":10601,"end":10618,"object":{"type":"Identifier","start":10601,"end":10613,"name":"documentRect"},"property":{"type":"Identifier","start":10614,"en